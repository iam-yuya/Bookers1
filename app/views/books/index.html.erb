
<p id="notice"></p>
<h1>Books</h1>

<table>

  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <tr>
      <td><%= @book.title %></td>
      <td><%= @book.body %></td>
      <td>
        <a class="" href="/books/show">Show</a>
      </td>

      <td>
        <a class="" href="/books/:id/edit">Edit</a>
      </td>

      <td>
        <a data-confirm="Are you sure?" class="" rel="nofollow" data-method="delete" href="/books/:id">
          Destoroy
        </a>
      </td>
    </tr>
  </tbody>

</table>






<%= form_with model: @book, url: '/books', method: :post do |f| %>

  <h4>Title</h4>
  <%= f.text_field :title %>

  <h4>body</h4>
  <%= f.text_area :body %>

  <%= f.submit 'Create Book' %>

<% end %>

















